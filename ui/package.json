{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react-scripts": "5.0.1",
    "react-redux": "^8.0.2",
    "react-bootstrap": "^2.4.0",
    "web-vitals": "^2.1.4",
    "@reduxjs/toolkit": "^1.8.3",
    "react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.5.2"
  },
  "scripts": {
    "generate": "node src/swagger-codegen.js --input .../shared/post-it.yaml --output ./src/generated/swagger --name post-it",
    "start": "react-scripts start",
    "build": "npm run generate && npm run lint && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts --ext .tsx --fix ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
